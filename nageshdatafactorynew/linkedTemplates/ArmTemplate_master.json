{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nageshdatafactorynew"
		},
		"AzureFileStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureFileStorage1'"
		},
		"ls_Blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_Blob'"
		},
		"ls_adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2'"
		},
		"ls_adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nageshadls.dfs.core.windows.net"
		},
		"ls_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://file-examples-com.github.io"
		},
		"Azure-SSISIR_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "nageshsqlserver.database.windows.net"
		},
		"Azure-SSISIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "v-nagpon"
		},
		"Azure-SSISIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"AzureFileStorageprivateendpoint_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/6b0a72ea-ca4d-488d-87f1-c5e515a2a7f4/resourceGroups/Nagesh_RG/providers/Microsoft.Storage/storageAccounts/nageshsa"
		},
		"AzureFileStorageprivateendpoint_properties_groupId": {
			"type": "string",
			"defaultValue": "file"
		},
		"AzureFileStorageprivateendpoint_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"nageshsa.file.core.windows.net"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nageshdatafactorynew_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_Blob_connectionString": {
						"value": "[parameters('ls_Blob_connectionString')]"
					},
					"ls_adlsgen2_accountKey": {
						"value": "[parameters('ls_adlsgen2_accountKey')]"
					},
					"AzureFileStorage1_connectionString": {
						"value": "[parameters('AzureFileStorage1_connectionString')]"
					},
					"ls_adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_properties_typeProperties_url')]"
					},
					"ls_http_properties_typeProperties_url": {
						"value": "[parameters('ls_http_properties_typeProperties_url')]"
					},
					"Azure-SSISIR_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('Azure-SSISIR_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"Azure-SSISIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('Azure-SSISIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"Azure-SSISIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('Azure-SSISIR_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					},
					"AzureFileStorageprivateendpoint_properties_privateLinkResourceId": {
						"value": "[parameters('AzureFileStorageprivateendpoint_properties_privateLinkResourceId')]"
					},
					"AzureFileStorageprivateendpoint_properties_groupId": {
						"value": "[parameters('AzureFileStorageprivateendpoint_properties_groupId')]"
					},
					"AzureFileStorageprivateendpoint_properties_fqdns": {
						"value": "[parameters('AzureFileStorageprivateendpoint_properties_fqdns')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}